# Local development example environment (360Â° coverage)
STAGE=dev
NODE_VERSION=22
NEXT_VERSION=15
NEXT_RUNTIME=node

# Secrets
AUTH_SECRET=your-random-secret-here

# Persistence (local Docker)
POSTGRES_URL=postgresql://user:password@localhost:5432/ai_ysh
REDIS_URL=redis://localhost:6379
BLOB_READ_WRITE_TOKEN=minioadmin:minioadmin@localhost:9000/ai-ysh

# AI Gateway & Providers (local)
AI_GATEWAY_API_KEY=local
XAI_API_KEY=
ANTHROPIC_API_KEY=
OPENAI_API_KEY=
GOOGLE_CLOUD_PROJECT_ID=
GOOGLE_CLOUD_LOCATION=us-central1
GOOGLE_APPLICATION_CREDENTIALS=
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_DEFAULT_MODEL=qwen3:30b

# Load balancer & performance
LOAD_BALANCER_COST_WEIGHT=30
LOAD_BALANCER_LATENCY_WEIGHT=40
LOAD_BALANCER_RELIABILITY_WEIGHT=30
PERFORMANCE_MONITORING_ENABLED=true
PERFORMANCE_METRICS_RETENTION_HOURS=168
PERFORMANCE_ALERT_THRESHOLD_LATENCY=5000
PERFORMANCE_ALERT_THRESHOLD_ERROR_RATE=0.05
PROVIDER_PRIORITY_ORDER=xai,ollama,anthropic,openai,google
MAX_COST_PER_REQUEST=0.10
MAX_LATENCY_MS=10000
PROVIDER_TIMEOUT_MS=30000

# Google Maps
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your-google-maps-key

# Bus & Channels (optional)
BUS_KIND=redis
BUS_URL=redis://localhost:6379
WHATSAPP_MCP_API_KEY=
TELEGRAM_BOT_TOKEN=
EMAIL_SMTP_URL=
SMS_PROVIDER_TOKEN=
VOICE_PROVIDER_TOKEN=
TWITTER_BEARER_TOKEN=

# Quotas & Feature Gates
DAILY_LIMIT_GUEST=20
DAILY_LIMIT_REGULAR=100
OWNER_WIZARD_ENABLED=true
OWNER_BATCH_ENABLED=false
INTEGRATOR_ADVANCED_ENABLED=true
INTEGRATOR_BATCH_ENABLED=true
